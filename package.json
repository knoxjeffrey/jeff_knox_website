{
  "private": true,
  "name": "jeff_knox_website",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all del:dist build:assets build:site",
    "dev": "npm run dev:assets & npm run dev:site",
    "prod": "npm run build:serve",
    "build:site": "ELEVENTY_ENV=production npx eleventy",
    "dev:site": "ELEVENTY_ENV=development npx eleventy --serve",
    "build:assets": "webpack --config webpack.config.prod.js",
    "dev:assets": "webpack --config webpack.config.dev.js",
    "build:serve": "serve ./dist/",
    "del:dist": "rimraf ./dist"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-htm": "^3.0.0",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "eslint": "^6.0.1",
    "eslint-config-synacor": "^3.0.4",
    "fibers": "^5.0.0",
    "html-minifier": "^4.0.0",
    "md5-file": "^5.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "module-alias": "^2.2.2",
    "node-sass": "^5.0.0",
    "node-sass-glob-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "serve": "^11.3.2",
    "sirv-cli": "^1.0.3",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.7.2"
  },
  "dependencies": {
    "htm": "^3.0.4",
    "preact": "^10.1.0",
    "preact-render-to-string": "^5.1.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE >= 11"
  ],
  "_moduleAliases": {
    "@components": "src/components",
    "@utils": "src/utils"
  }
}
